{
  "name": "@geniius/gedcom-core",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.6.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "pnpm run clean && tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint ."
  },
  "dependencies": {
    "@geniius/schema": "workspace:*"
  },
  "devDependencies": {
    "rimraf": "^6.1.2",
    "typescript": "^5.9.2"
  }
}
